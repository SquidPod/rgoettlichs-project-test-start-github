  digitalWrite(LED_BUILTIN, LOW);
  delay(lng);

  digitalWrite(LED_BUILTIN, HIGH);  
  digitalWrite(LED_BUILTIN, LOW);
  delay(shrt);
  digitalWrite(LED_BUILTIN, HIGH); 
  delay(shrt);
  digitalWrite(LED_BUILTIN, LOW);
  delay(shrt);
  digitalWrite(LED_BUILTIN, HIGH);  
  digitalWrite(LED_BUILTIN, LOW);
  delay(lng);

  digitalWrite(LED_BUILTIN, HIGH);  
  digitalWrite(LED_BUILTIN, LOW);
  delay(shrt);
  digitalWrite(LED_BUILTIN, HIGH); 
  delay(shrt);
  digitalWrite(LED_BUILTIN, LOW);
  delay(shrt);
  digitalWrite(LED_BUILTIN, HIGH);  
  digitalWrite(LED_BUILTIN, LOW);
  delay(shrt);
  digitalWrite(LED_BUILTIN, HIGH);  
  digitalWrite(LED_BUILTIN, LOW);
  delay(shrt);

